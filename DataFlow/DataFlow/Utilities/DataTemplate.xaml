                    xmlns:vm="clr-namespace:DataFlow.ViewModel"
                    xmlns:v="clr-namespace:DataFlow.View">
    
    <DataTemplate DataType="{x:Type vm:FilesViewModel}">
        <v:FilesPage />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:TableViewModel}">
        <v:TablePage />
    </DataTemplate>

</ResourceDictionary>